<!DOCTYPE html>
<html>
<head>
  <title>Fastehr</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <a class="brand" href="#">Fast-EHR</a>
      </div> <!-- Container closing div -->
    </div> <!-- navbar-inner closing div -->
  </div>  <!-- navbar-fixed-top -->
  <div class="container">
    <div class="content">
      <div class="wrapper">
        <div class="proper-content">
          <div class="row">
            <div class="span12">
              <%= yield %>
            </div> <!-- span12 -->
          </div> <!-- row -->
        </div> <!-- proper-content -->

        <div class="push"></div>
      </div><!-- /.wrapper -->
      <div class="footer-wrapper">
        <footer>
          <hr>
          <div class="row">
            <div class="span2">
              <%= button_to "   claim    ", pages_claim_path, :method => :get, :class => "btn btn-primary btn-large"  %>
            </div>
            <div class="span2">
              <%= button_to "annotation", pages_annotation_path, :method => :get, :class => "btn btn-primary btn-large"  %>
            </div>
            <div class="span2">
              <%= button_to "decision", pages_decision_path, :method => :get, :class => "btn btn-primary btn-large"  %>
            </div>
          </div>
          <hr>
          </footer>
      </div>
    </div> <!-- content -->
  </div>

</body>
</html>






